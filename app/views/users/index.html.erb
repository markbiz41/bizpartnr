<div class='row'>
  <div class='col-md-12'>

    <h1>Partner List</h1>

    <table class="table table-striped">
      <thead>
        <tr>
          <th>Full Name</th>
          <th>Email </th>
          <th>Industry</th>
          <th>Position</th>
          <th>City</th>
          <th>State</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.full_name, user %></td>
            <td><%= user.email %></td>
            <td><%= user.industry.try(:name) %></td>
            <td><%= user.position %></td>
            <td><%= user.city %></td>
            <td><%= user.state %></td>

            </tr>
        <% end %>
      </tbody>
    </table>

  </div>
</div>



<%= paginate @users, :theme => 'twitter-bootstrap-3',
                   :pagination_class => "pagination-sm pagination-centered" %>
<br>
